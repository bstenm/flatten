{
  "name": "flatten",
  "version": "1.0.0",
  "description": "Scripts to flatten and array of any depth",
  "repository": {
    "type": "git",
    "url": "https://github.com/bstenm/flatten"
  },
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "watch-ts": "tsc -w",
    "watch-node": "NODE_ENV=development nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "pretest": "yarn lint",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "NODE_ENV=test jest --watch",
    "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
  },
  "author": "Bertrand Steinmetz",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "23.3.9",
    "@types/lodash": "4.14.118",
    "graphql-request": "1.8.2",
    "jest": "23.6.0",
    "nodemon": "1.18.6",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.1.6"
  },
  "dependencies": {
    "concurrently": "4.1.0",
    "graphql-yoga": "1.16.7",
    "lodash": "4.17.11"
  }
}
